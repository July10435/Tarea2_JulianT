<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tarea2_JulianT.Page1">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtUsuario" Placeholder="Usuario"/>
            <Entry x:Name="txtContraseña" Placeholder="Contraseña" IsPassword="True"/>
            <Button x:Name="vtnIngresar" Text="Ingresar" Clicked="vtnIngresar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>